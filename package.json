{
  "name": "bitergo-common-design-system",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "eslint --ext .ts bitergo-common",
    "build:lib": "ng build bitergo-common --configuration production",
    "pack:lib": "npm run build:lib && cd dist/bitergo-common && npm pack && cd.. && cd..",
    "publish": "npm run pack:lib && shx cp bitergo-common/.npmrc dist/bitergo-common && cd dist/bitergo-common && npm publish && cd.. && cd..",
    "npmrc-builder": "node npmrc-builder.js",
    "build:pack": "npm run build:lib && npm run pack:lib"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^12.2.11",
    "@angular/common": "^12.2.11",
    "@angular/compiler": "^12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/forms": "^12.2.11",
    "@angular/platform-browser": "^12.2.11",
    "@angular/platform-browser-dynamic": "^12.2.11",
    "@angular/router": "^12.2.11",
    "@nguniversal/express-engine": "^12.1.2",
    "@ngx-translate/core": "^13.0.0",
    "@types/express": "^4.17.13",
    "aos": "^2.3.4",
    "express": "^4.15.2",
    "langs-cookie-consent": "0.0.15",
    "langs-navigation": "0.0.13",
    "ngx-matomo": "^1.0.0-rc.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
